@model IEnumerable<EDIFileStoreWebApp.Models.watchlist>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ISA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GS)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ST)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.B4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IEA)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ISA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GS)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ST)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.B4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IEA)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
